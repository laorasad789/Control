<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Menú Administrador{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('css/materialize.min.css') }}">
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        
        {% block head %}
    <nav>
      <div class="nav-wrapper cyan darken-1">
      <a href="#" class="brand-logo right cyan darken-1">Logout</a>
      <ul id="nav-mobile" class="left hide-on-med-and-down cyan darken-1">
        <li><a href="#">Inicio</a></li>
        <li><a href="{{ path('Clasificacion') }}">Clasificación</a></li>
        <li><a href="{{ path('Marca') }}">Marca</a></li>
        <li><a href="#">Bitácora</a></li>
        <li><a href="#">Reportes</a></li>
      </ul>
      </div>
    </nav>
        {% endblock %}
        {% block body %}
            <div id="contenido" style="margin-top: 5%;">
                {% block contenido %}

                {% endblock %}
            </div>
    
        {% endblock %}
        {% block javascripts %}
            <script type="application/javascript" src="{{ asset('js/jquery-3.1.0.js') }}"></script>
            <script type="application/javascript" src="{{ asset('js/materialize.min.js') }}"></script>
            <script>
                $(document).ready(function () {
                    $(".dropdown-button").dropdown();
                });
            </script>
        {% endblock %}
    </body>
</html>
